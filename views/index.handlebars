
<!--  page-wrapper -->
        <div id="page-wrapper">

            <div class="row">
                <!-- Page Header -->
                <div class="col-lg-12">
                    <h1 class="page-header">Client Admin Dashboard</h1>
                    <button id="test">CLICK ME!</button>
                </div>
                <!--End Page Header -->
            </div>

            <div class="row">
                <!-- Welcome -->
                <div class="col-lg-12">
                    <div class="alert alert-info">
                        <i class="fa fa-folder-open"></i><b>&nbsp;Hello! </b>Welcome Back <b>Jonny Deen </b>
                    </div>
                </div>
                <!--end  Welcome -->
            </div>

<!--quick info section -->

            <div class="row">
                
                <div class="col-lg-3">
                    <div class="alert alert-danger text-center">
                        <i class="fa fa-money fa-3x"></i>
                            <br>
                                <b>Cost</b>
                                    <br>
                                        <div id="cost">$3000</div>
                    </div>
                </div>

                <div class="col-lg-3">
                    <div class="alert alert-success text-center">
                        <i class="fa  fa-usd fa-3x"></i>
                            <br>
                                <b>CPM</b>
                                    <br>
                                        <div id="cpm">
                                            {{#if cpm}}

                                            <p> {{this.cpm}} </p>

                                            {{/if}}
        

                                        </div>
                    </div>
                </div>

                <div class="col-lg-3">
                    <div class="alert alert-info text-center">
                         <i class="fa fa-bar-chart-o fa-3x"></i>
                            <br>
                                <b>ROI</b>
                                    <br>
                                        <div id="roi">

                                         {{#if roi}}

                                            <p> {{this.roi}}% </p>

                                            {{/if}}
        

                                        </div>
                    </div>
                </div>
             
            </div>


            <div class="row">
                <div class="col-lg-8">



                    <!--Area chart example -->
                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <i class="fa fa-bar-chart-o fa-fw"></i>   Influencer Comparison
                            <div class="pull-right">
                              
                            </div>
                        </div>

                        <div class="panel-body">
                            <div id="morris-donut-chart"></div>
                        </div>

                    </div>
                    <!--End area chart example -->
                    
                    <!--Simple table example -->

                    <div class="panel panel-primary">
                        <div class="panel-heading">
                            <i class="fa fa-bar-chart-o fa-fw"></i>   Influencer Data
                            <div class="pull-right">
                           
                            </div>
                        </div>

                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="table-responsive">
                                        <table class="table table-bordered table-hover table-striped">
                                            <thead>
                                               
                                                <tr>
                                                    <th>Influencer</th>
                                                    <th>Followers</th>
                                                    <th>Total Views</th>
                                    
                                                </tr>
                                            </thead>
                                            <tbody>
                                                 
                                                 {{#each users}}

                                                    {{#if this.active_user}}

                                                        <tr>
                                                            <td>{{this.display_name}}</td>
                                                            <td>{{this.followers}}</td>
                                                            <td>{{this.views}}</td>
                                                        </tr>

                                                    {{/if}}

                                                {{/each}}

                                            </tbody>
                                        </table>
                                    </div>

                                </div>

                            </div>
                            <!-- /.row -->
                        </div>
                        <!-- /.panel-body -->
                    </div>

                    <!--End simple table example -->

                 </div>

                <div class="col-lg-4">
                    <div class="panel panel-primary text-center no-boder">
                        <div class="panel-body yellow">
                            <i class="fa fa-users fa-3x"></i>
                            <h3 id="total-influencers">
                             {{#if users}}
                        
                                <p><strong> {{../users.length}}</strong></p>

                            {{/if}}


                            </h3>
                        </div>
                        <div class="panel-footer">
                            <span class="panel-eyecandy-title">Total Influencers
                            </span>
                        </div>
                    </div>
                    <div class="panel panel-primary text-center no-boder">
                        <div class="panel-body blue">
                            <i class="fa fa-bullhorn fa-3x"></i>
                            <h3 id="total-impressions">
                               {{#if total_views}}

                                 

                                    <p> {{this.total_views}} </p>
           

                                {{/if}}

                             </h3>
                        </div>
                        <div class="panel-footer">
                            <span class="panel-eyecandy-title">Total Impressions
                            </span>
                        </div>
                    </div>
                    <div class="panel panel-primary text-center no-boder">
                        <div class="panel-body green">
                            <i class="fa fa-thumbs-o-up fa-3x"></i>
                            <h3 id="average-daily-impressions">

                            {{#each users}}

                                 {{#if this.active_user}}

                                    <p> {{this.game}} </p>

                                 {{/if}}

                          {{/each}}
                            </h3>
                        </div>
                        <div class="panel-footer">
                            <span class="panel-eyecandy-title">Top Games Played
                            </span>
                        </div>
                    </div>
                </div>

            </div>

                </div>
                <div class="col-lg-4">
                    
                </div>
                <div class="col-lg-4">
                    
                </div>
            </div>


         


        </div>
        <!-- end page-wrapper -->

